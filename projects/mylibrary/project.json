{
  "name": "mylibrary",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "projects/mylibrary/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "projects/mylibrary/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/projects/mylibrary"
      ],
      "options": {
        "jestConfig": "projects/mylibrary/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
